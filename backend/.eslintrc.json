{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": 1,
        "no-empty": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn"
    }
}
